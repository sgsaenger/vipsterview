{
    "name": "vipster-js-widget",
    "version": "0.1.0",
    "description": "Jupyter widget for Vipster",
    "author": "Sebastian GsÃ¤nger",
    "license": "GPL-3.0-only",
    "repository": {
        "type": "git",
        "url": "https://github.com/sgsaenger/vipster"
    },
    "bugs": "https://github.com/sgsaenger/vipster/issues",
    "homepage": "https://sgsaenger.github.io/vipster",
    "keywords": [
        "chemistry",
        "molecular graphics",
        "molecular structure",
        "visualization",
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension",
        "ipython",
        "ipywidgets"
    ],
    "scripts": {
        "prepare": "npm run build:lab && cp node_modules/vipster/dist/vipster.js .",
        "build": "npm run build:nb && npm run build:lab",
        "build:nb": "cat src/ext-head.js src/core.js src/ext-tail.js > extension.js",
        "build:lab": "head -c -1 src/lab-head.js | cat - src/core.js > index.js",
        "update": "npm install && npm run build:nb && cp extension.js node_modules/vipster/dist/vipster.js ../vipsterview/static"
    },
    "devDependencies": {
        "vipster": ">=1.19.0"
    },
    "main": "index.js",
    "files": [
        "vipster.js",
        "lab-plugin.js"
    ],
    "jupyterlab": {
        "extension": "lab-plugin"
    }
}
